{
  "name": "tell",
  "version": "1.0.0",
  "description": "tell_cloud",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.1.1",
    "egg-scripts": "^2",
    "jsonwebtoken": "^8.5.1",
    "pkg": "^5.8.0",
    "uuid": "^9.0.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "egg-bin": "^5.5.0",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-tell",
    "stop": "egg-scripts stop --title=egg-server-tell",
    "dev": "egg-bin dev --port 700",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "build": "pkg . --out-path D:/ --debug",
    "bin": "server.js"
  },
  "pkg": {
    "scripts": [
        "./app/**/*.js",
      "./config/*.js",
      "./node_modules/nanoid/**/*.js",
      "./config/*"
    ],
    "assets": [
      "./app/public/**/*"
    ],
    "targets": [
      "node12-win-x64"
    ]
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "songshilian",
  "license": "MIT"
}